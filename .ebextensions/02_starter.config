container_commands:
  01_migrate:
    command: "python api-task/manage.py migrate"
    leader_only: true
  02_collectstatic:
    command: "python api-task/manage.py collectstatic --noinput"
  03_createsu:
    command: "python api-task/manage.py createsu"
    leader_only: true
option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "taskmanager.settings"